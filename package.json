{
  "name": "bevyrly",
  "displayName": "bevyrly",
  "description": "It's pronounced beverly. A bevy-oriented coding context editor",
  "version": "0.0.1",
  "repository": {
    "url": "https://github.com/devlike-code/bevyrly"
  },
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Notebooks",
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "virtualWorkspaces": {
      "supported": true
    },
    "commands": [
      {
        "command": "bevyrly.newQuery",
        "title": "bevyrly: New Query"
      },
      {
        "command": "bevyrly.newNotebook",
        "title": "bevyrly: New Notebook"
      },
      {
        "command": "bevyrly.start",
        "title": "bevyrly: Start Analyzer"
      }
    ],
    "notebooks": [
      {
        "type": "bevyrly-notebook",
        "displayName": "Bevyrly Query Notebook",
        "selector": [
          {
            "filenamePattern": "*.bevyrly"
          }
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.86.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "jinx-rust": "^0.1.6",
    "vscode-languageclient": "^9.0.1"
  }
}